<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ArtMarketplace">
        <activity
            android:name="com.example.artmarketplace.ui.provider.ProviderOrdersActivity"
            android:exported="false" />
        <activity
            android:name="com.example.artmarketplace.ui.provider.EditListingActivity"
            android:exported="false" />
        <activity
            android:name="com.example.artmarketplace.ui.provider.CreateListingActivity"
            android:exported="false" />
        <activity
            android:name="com.example.artmarketplace.ui.provider.ProviderHomeActivity"
            android:exported="false" />
        <activity
            android:name="com.example.artmarketplace.ui.customer.ArtDetailActivity"
            android:exported="false" />
        <activity
            android:name="com.example.artmarketplace.ui.customer.ReceiptActivity"
            android:exported="false" />
        <activity
            android:name="com.example.artmarketplace.ui.customer.CheckoutActivity"
            android:exported="false" />
        <activity
            android:name="com.example.artmarketplace.ui.customer.CartActivity"
            android:exported="false" />
        <activity
            android:name="com.example.artmarketplace.ui.customer.CustomerHomeActivity"
            android:exported="false" />
        <activity
            android:name="com.example.artmarketplace.ui.chat.ChatActivity"
            android:exported="false" />
        <activity
            android:name="com.example.artmarketplace.ui.RoleSelectActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
